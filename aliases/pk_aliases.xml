<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE MudletPackage>
<MudletPackage version="1.001">
	<AliasPackage>
		<AliasGroup isActive="yes" isFolder="yes">
			<name>PK</name>
			<script></script>
			<command></command>
			<packageName></packageName>
			<regex></regex>
			<Alias isActive="yes" isFolder="no">
				<name>Change Wimpy</name>
				<script>send("change wimpy "..matches[2])</script>
				<command></command>
				<packageName></packageName>
				<regex>^cw (.*)$</regex>
			</Alias>
			<Alias isActive="yes" isFolder="no">
				<name>Bash</name>
				<script>if not matches[3] then
  send("bash")
else
  send("bash h."..matches[4])
end</script>
				<command></command>
				<packageName></packageName>
				<regex>(^b$)|(^b (\S+)$)</regex>
			</Alias>
