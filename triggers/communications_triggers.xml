				<name>Rat and Raven Reports</name>
				<script>selectCaptureGroup(1)
communications:append("Misc")

local roomname = matches[4]
local foundrooms = searchRoom(roomname, true, true)

if table.is_empty(foundrooms) then
    selectCaptureGroup(4)
    setLink(function() end, "No matches found!")
elseif table.size(foundrooms) == 1 then
  for id, name in pairs(foundrooms) do
    selectCaptureGroup(4)
    setLink(function() centerview(id) end, "Click to center map on room.")
  end
else
  selectCaptureGroup(4)
  
  setLink(function() 
    for id, name in spairs(foundrooms) do
      local zone = getRoomUserData(id, "zone")
      echoLink(
        "Room ID: {id}, zone: {zone}\n", 
        function() centerview(id) end, 
        "Click to center map on room.",
        true
      )
    end 
  end, "Click to display rooms with matching name.")
  
end</script>
