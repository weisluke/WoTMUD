				<name>Stats</name>
				<script>local str = tonumber(matches[2])
local int = tonumber(matches[3])
local wil = tonumber(matches[4])
local dex = tonumber(matches[5])
local con = tonumber(matches[6])
local sum = str + int + wil + dex + con

if
  (str &lt; statting.stats.str or
  int &lt; statting.stats.int or
  wil &lt; statting.stats.wil or
  dex &lt; statting.stats.dex or
  con &lt; statting.stats.con) and sum &lt; statting.stats.sum
then
  statting:save_stats(str, int, wil, dex, con, sum)
  sendAll("disengage", "restat", "k sapling", "wield "..statting.weapon, "score")
else
  statting:save_stats(str, int, wil, dex, con, sum)
  local msg =
    "Congratulations! You have statted a character with your set minimum stats or a sufficient statsum. You now have a " ..
    str ..
    " " ..
    int ..
    " " ..
    wil ..
    " " ..
    dex ..
    " " ..
    con ..
    " " ..
    statting.sex ..
    " " ..
    statting.race ..
    " " ..
    statting.class ..
    " character."
  statting:echo("\n&lt;green&gt;" .. msg .. "\n")
  showNotification("WoTMUD Statting", msg)
  statting.statting = false
  disableTrigger("Statting")
end</script>
